{
  "expo": {
    "name": "my-adventurous-weather",
    "slug": "my-adventurous-weather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSMotionUsageDescription": "Needs Permission for Steps"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#208BE8"
      },
      "package": "com.gemib.myadventurousweather"
    },
    "plugins": [
      "./node_modules/expo-sample-pedometer/expo-sample-pedometer-plugin.js",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
